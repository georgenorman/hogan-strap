
<div class="row featurette">
  <div class="col-md-5">
    <img class="featurette-image img-responsive" src="{{imageSrc}}" alt="Generic placeholder image">
  </div>
  <div class="col-md-7">
    <h2 class="featurette-heading">{{heading}} <span class="text-muted">{{headingMuted}}</span></h2>
    <p class="lead">{{content}}</p>
  </div>
</div>

<hr class="featurette-divider">
